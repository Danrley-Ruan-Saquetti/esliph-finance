{
  "compilerOptions": {
    "forceConsistentCasingInFileNames": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "strict": true,
    "types": [
      "nativewind/types"
    ],
    "baseUrl": ".",
    "paths": {
      "@router/*": [
        "./src/app/*"
      ],
      "@assets/*": [
        "./src/assets/*"
      ],
      "@server/*": [
        "./src/server/*"
      ],
      "@components/*": [
        "./src/components/*"
      ],
      "@services/*": [
        "./src/services"
      ],
      "@util/*": [
        "./src/util/*"
      ],
    }
  },
  "extends": "expo/tsconfig.base"
}
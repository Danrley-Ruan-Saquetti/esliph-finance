{
  "compilerOptions": {
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "types": [
      "nativewind/types"
    ],
    "baseUrl": ".",
    "paths": {
      "@router/*": [
        "./app/*"
      ],
      "@assets/*": [
        "./assets/*"
      ],
      "@server/*": [
        "./server/*"
      ],
      "@components/*": [
        "./components/*"
      ],
      "@services/*": [
        "./services"
      ],
      "@util/*": [
        "./util/*"
      ],
    }
  },
  "extends": "expo/tsconfig.base"
}
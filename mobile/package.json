{
  "name": "portal-finance",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "start:clear": "expo start --clear",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build:ios": "eas build --platform ios --clear-cache --profile preview",
    "build:android": "eas build --platform android --clear-cache --profile preview",
    "submit:android": "eas submit --platform android --clear-cache --profile preview",
    "submit:ios": "eas submit --platform ios --clear-cache --profile preview",
    "build:all": "eas build --platform all --clear-cache --profile preview",
    "doctor": "npx -y expo-doctor",
    "expo:update": "npx expo install --fix",
    "script:update": "node scripts/update.js",
    "script:setup": "bash scripts/setup.sh"
  },
  "dependencies": {
    "@esliph/common": "^1.0.2",
    "@esliph/console": "^1.0.3",
    "@esliph/decorator": "^1.0.2",
    "@esliph/http": "^1.0.2",
    "@esliph/injection": "^1.0.6",
    "@esliph/module": "^1.0.1",
    "@expo/config-plugins": "^7.2.2",
    "base-64": "^1.0.0",
    "expo": "~49.0.15",
    "expo-constants": "~14.4.2",
    "expo-crypto": "~12.4.1",
    "expo-linking": "~5.0.2",
    "expo-router": "^2.0.0",
    "expo-sqlite": "~11.3.3",
    "expo-status-bar": "~1.6.0",
    "nativewind": "^2.0.11",
    "react": "18.2.0",
    "react-native": "0.72.6",
    "react-native-gesture-handler": "~2.12.0",
    "react-native-safe-area-context": "4.6.3",
    "react-native-screens": "~3.22.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/base-64": "^1.0.2",
    "@types/react": "~18.2.14",
    "babel-plugin-module-resolver": "^5.0.0",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.1.3"
  },
  "private": true,
  "overrides": {
    "metro": "0.76.0",
    "metro-resolver": "0.76.0"
  }

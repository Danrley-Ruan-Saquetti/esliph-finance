{
    "expo": {
        "name": "portal-finance",
        "slug": "portal-finance",
        "scheme": "portal-finance",
        "owner": "esliph",
        "jsEngine": "hermes",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./src/assets/icon.png",
        "userInterfaceStyle": "light",
        "splash": {
            "image": "./src/assets/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "experiments": {
            "tsconfigPaths": true
        },
        "assetBundlePatterns": [
            "**/*"
        ],
        "ios": {
            "supportsTablet": true
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./src/assets/adaptive-icon.png",
                "backgroundColor": "#ffffff"
            },
            "package": "com.esliph.portalfinance"
        },
        "web": {
            "favicon": "./src/assets/favicon.png"
        },
        "plugins": [
            "expo-router"
        ],
        "extra": {
            "router": {
                "origin": false
            },
            "eas": {
                "projectId": "940a4371-7789-4e4e-92fc-ee3a90d1ebb4"
            }
        }
    }
}